         MiniLogo="https://cdn.masastack.com/stack/images/logo/MASAStack/logo.png">
    @Body
</SLayout>

@code {
    List<string> _whiteUris = new();

    public MainLayout()
    {
        _whiteUris = new List<string> { "http://www.baidu.com", "/users/*" };
    }

    private bool SignOut()
    {
        return false;
    }

}
