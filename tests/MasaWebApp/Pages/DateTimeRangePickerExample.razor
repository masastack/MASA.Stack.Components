@page "/dateTimeRangePickerExample"

<div class="pa-12 mb-6">
    <DateTimePicker Value=StartTime Max=EndTime />
</div>

<div class="pa-12 mb-6">
    <DateTimeRangePicker @bind-StartTime=StartTime @bind-EndTime=EndTime />
</div>

<div class="pa-12 mb-6">
    <RelativeTime @bind-StartTime="StartTime"  @bind-EndTime="EndTime" />
</div>

@code{
    public DateTime? StartTime { get; set; }
    public DateTime? EndTime { get; set; }
}